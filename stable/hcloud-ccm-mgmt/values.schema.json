{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nameOverride": {
      "type": "string"
    },
    "fullnameOverride": {
      "type": "string"
    },
    "replicaCount": {
      "type": "integer"
    },
    "securityContext": {
      "type": "object"
    },
    "podSecurityContext": {
      "type": "object",
      "properties": {
        "controller": {
          "type": "object"
        }
      }
    },
    "image": {
      "type": "object",
      "properties": {
        "pullSecrets": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "controller": {
          "type": "object",
          "properties": {
            "repository": {
              "type": "string"
            },
            "tag": {
              "type": "string"
            },
            "pullPolicy": {
              "type": "string"
            }
          },
          "required": [
            "repository",
            "tag",
            "pullPolicy"
          ]
        }
      }
    },
    "resources": {
      "type": "object",
      "properties": {
        "controller": {
          "type": "object",
          "properties": {
            "limits": {
              "type": "object",
              "properties": {
                "cpu": {
                  "type": "string"
                },
                "memory": {
                  "type": "string"
                }
              }
            },
            "requests": {
              "type": "object",
              "properties": {
                "cpu": {
                  "type": "string"
                },
                "memory": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "serviceAccount": {
      "type": "object",
      "properties": {
        "create": {
          "type": "boolean"
        },
        "name": {
          "type": "string"
        },
        "annotations": {
          "type": "object"
        }
      }
    },
    "updateStrategy": {
      "type": "object"
    },
    "labels": {
      "type": "object"
    },
    "annotations": {
      "type": "object"
    },
    "extraInitContainers": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "extraSidecarContainers": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "extraVolumes": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "extraVolumeMounts": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "service": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "annotations": {
          "type": "object"
        },
        "labels": {
          "type": "object"
        }
      }
    },
    "metrics": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "port": {
          "type": "integer"
        },
        "internalPort": {
          "type": "integer"
        },
        "serviceMonitor": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            },
            "namespace": {
              "type": [
                "string",
                "null"
              ]
            },
            "jobLabel": {
              "type": [
                "string",
                "null"
              ]
            },
            "interval": {
              "type": [
                "string",
                "null"
              ]
            },
            "scrapeTimeout": {
              "type": [
                "string",
                "null"
              ]
            },
            "honorLabels": {
              "type": "boolean"
            },
            "metricRelabelings": {
              "type": "array",
              "items": {
                "type": "object"
              }
            },
            "relabelings": {
              "type": "array",
              "items": {
                "type": "object"
              }
            },
            "annotations": {
              "type": "object"
            },
            "labels": {
              "type": "object"
            }
          }
        }
      }
    },
    "nodeSelector": {
      "type": "object"
    },
    "affinity": {
      "type": "object"
    },
    "tolerations": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "extraEnvVariables": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "config": {
      "type": "object",
      "properties": {
        "kubeconfig": {
          "type": [
            "string",
            "null"
          ]
        },
        "network": {
          "type": [
            "string",
            "null"
          ]
        },
        "clusterCidr": {
          "type": "string"
        }
      }
    }
  },
  "required": []
}
